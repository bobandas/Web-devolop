<section>
    <div class="container">
        
        <a href="/admin/add-products" class="btn btn-info btn-rounded float-right m-3" role="button">Add user role</a>
        <!--Table-->
        <table class="table table-hover table-fixed">

            <!--Table head-->
            <thead>
                <tr>
                    <th>No</th>
                    <th>User</th>
                    <th>User Name</th>
                    <th>Email</th>
                    <th>Role</th>
                    <th>Password</th>
                    <th>Action</th>
                </tr>
            </thead>
            <!--Table head-->

            <!--Table body-->
            <tbody>
                {{#each users}}
                <tr>
                    <th scope="row">4</th>
                    <td>{{this.fullName}}</td>
                    <td>{{this.username}}</td>
                    <td>{{this.email}}</td>
                    <td>{{this.role}}</td>
                    <td>{{this.password}}</td>
                    <td>41</td>
                </tr>
                {{/each}}
               

            </tbody>
            <!--Table body-->

        </table>
        <!--Table-->
    </div>

</section>